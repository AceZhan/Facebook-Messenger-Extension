{
	"name": "QuickMessenger",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Displays Facebook Messenges on your toolbar",
	"icons": {"128": "icon.png"},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Your Facebook Messenges",
		"default_popup": "popup.html"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}